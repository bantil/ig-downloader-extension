{
    "description": "Downloads images in full-res from instagram.",
    "manifest_version": 2,
    "name": "ig-downloader",
    "version": "1.0",
    "homepage_url": "https://github.com/dynalar",
    "icons": {
        "48": "assets/icons/ig-downloader.png"
    },
    "permissions": [
        "webNavigation",
        "*://www.instagram.com/*"
    ],
    "content_scripts": [
        {
            "matches": ["*://www.instagram.com/*"],
            "js": ["main.js"],
            "run_at": "document_idle"
        }
    ]
}
  